<!DOCTYPE html>
<html lang="en">

	<head>

		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<meta name="description" content="">
		<meta name="author" content="">
		<link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">

		<title>Fitnatic HOME PAGE</title>

		<!-- Additional CSS Files -->
		<link rel="stylesheet" type="text/css" href="{{ asset('user/assets/css/bootstrap.min.css') }}">
		<link rel="stylesheet" type="text/css" href="{{ asset('user/assets/css/font-awesome.css') }}">
		<link rel="stylesheet" href="{{ asset('user/assets/css/templatemo-training-studio.css') }}">

	</head>

	<body>

		<!-- ***** Preloader Start ***** -->
		<div id="js-preloader" class="js-preloader">
			<div class="preloader-inner">
				<span class="dot"></span>
				<div class="dots">
					<span></span>
					<span></span>
					<span></span>
				</div>
			</div>
		</div>
		<!-- ***** Preloader End ***** -->


		<!-- ***** Header Area Start ***** -->
		<header class="header-area header-sticky">
			<div class="container">
				<div class="row">
					<div class="col-12">
						<nav
							class="main-nav">
							<!-- ***** Logo Start ***** -->
							<a href="index.html" class="logo">Fit<em>
									Natic</em>
							</a>
							<!-- ***** Logo End ***** -->
							<!-- ***** Menu Start ***** -->
							<ul class="nav">
								<li class="scroll-to-section">
									<a href="#top">Home</a>
								</li>
								<li class="scroll-to-section">
									<a href="#features">Activities</a>
								</li>
								<li class="scroll-to-section">
									<a href="#our-classes" class="active">Competitions</a>
								</li>
								<li class="scroll-to-section">
									<a href="#schedule">Forum</a>
								</li>
								<li class="scroll-to-section">
									<a href="#contact-us">Food Monitoring</a>
								</li>

							</ul>
							<a class='menu-trigger'>
								<span>Menu</span>
							</a>
							<!-- ***** Menu End ***** -->
						</nav>
					</div>
				</div>
			</div>
		</header>
		<!-- ***** Header Area End ***** -->

		<section class="section" id="schedule">
			<div class="container">
				<div class="row">
					<div class="col-lg-6 offset-lg-3">
						<div class="section-heading dark-bg">
                        
							<div id="ratingFormContainer" style="display: flex; flex-direction: column; align-items: center; text-align: center; margin-top: 120px;">
								<h2>Rate the
									<em>Competition</em>
								</h2>
								{{ form_start(ratingForm, {'attr': {'class': 'rating-form', 'id': 'ratingForm'}}) }}
								<div class="form-group">
									{{ form_label(ratingForm.Rating, 'Rating', {'label_attr': {'class': 'form-label', 'style': 'color: white;'}}) }}
									{{ form_widget(ratingForm.Rating, {'attr': {'class': 'form-control', 'style': 'width: 200px;'}}) }}
								</div>
								<div class="form-group">
									{{ form_label(ratingForm.Comment, 'Comment', {'label_attr': {'class': 'form-label', 'style': 'color: white;'}}) }}
									{{ form_widget(ratingForm.Comment, {'attr': {'class': 'form-control', 'style': 'width: 200px; height: 100px;', 'id': 'Comment'}}) }}
								</div>

								{{ form_row(ratingForm.save, {'label': 'Submit', 'attr': {'class': 'btn btn-primary', 'style': 'margin-top: 10px;', 'onclick': 'submitForm()'}}) }}
								{{ form_end(ratingForm) }}
							</div>
							{% for flashError in app.flashes('error') %}
								<div class="alert alert-danger" role="alert" style="margin-top: 30px; width: 600px; margin-left: auto; margin-right: auto;">
									{{ flashError }}
								</div>
							{% endfor %}
						</div>
					</div>
				</div>
			</div>
		</section>

		<!-- ***** Footer Start ***** -->
		<footer>
			<div class="container">
				<div class="row">
					<div class="col-lg-12">
						<p>
							Copyright &copy; - Designed by
							<a rel="nofollow" href="https://templatemo.com" class="tm-text-link" target="_parent">Fitnatic Team</a><br>
						</p>
					</div>
				</div>
			</div>
		</footer>

		<!-- jQuery -->
		<!-- Include CSS stylesheets -->
		<link rel="stylesheet" type="text/css" href="{{ asset('user/assets/css/bootstrap.min.css') }}">
		<link rel="stylesheet" type="text/css" href="{{ asset('user/assets/css/font-awesome.css') }}">
		<link
		rel="stylesheet" href="{{ asset('user/assets/css/templatemo-training-studio.css') }}">

		<!-- Include JavaScript libraries -->
		<script src="{{ asset('user/assets/js/jquery-2.1.0.min.js') }}"></script>

		<!-- Bootstrap -->
		<script src="{{ asset('user/assets/js/popper.js') }}"></script>
		<script src="{{ asset('user/assets/js/bootstrap.min.js') }}"></script>

		<!-- Plugins -->
		<script src="{{ asset('user/assets/js/scrollreveal.min.js') }}"></script>
		<script src="{{ asset('user/assets/js/waypoints.min.js') }}"></script>
		<script src="{{ asset('user/assets/js/jquery.counterup.min.js') }}"></script>
		<script src="{{ asset('user/assets/js/imgfix.min.js') }}"></script>
		<script src="{{ asset('user/assets/js/mixitup.js') }}"></script>
		<script src="{{ asset('user/assets/js/accordions.js') }}"></script>

		<!-- Global Init -->
		<script src="{{ asset('user/assets/js/custom.js') }}"></script>

	</body>

</html>
